    &END CELL
    &TOPOLOGY
            COORD_FILE_NAME pos.xyz
            COORD_FILE_FORMAT xyz
    &END TOPOLOGY 
    &VELOCITY
	@INCLUDE velocity.init
	PINT_UNIT bohr*au_t^-1
    &END VELOCITY
  &END SUBSYS
&END FORCE_EVAL

&FORCE_EVAL
!write nnp force eval here
   METHOD  APT
   &APT
	APT_TORCH_FILE nameoftorchfile.torch
	E_FIELD_VECTOR 0 0 5e-4
	&PRINT

	&TENSORS
	&EACH
		MD 1
	&END EACH
	&END TENSORS

	&FORCES
	&EACH
		MD 1
	&END EACH
	&END FORCES

	&STDS
	&EACH
		MD 1
	&END EACH
	&END STDS

	&END PRINT
   &END APT
  &SUBSYS
    &Cell
	ABC 1.5662700000000001E+01 1.5662700000000001E+01 1.5662700000000001E+01 
    &END CELL
    &TOPOLOGY
            COORD_FILE_NAME pos.xyz
            COORD_FILE_FORMAT xyz
    &END TOPOLOGY 
    &VELOCITY
	@INCLUDE velocity.init
	PINT_UNIT bohr*au_t^-1
    &END VELOCITY
  &END SUBSYS
&END FORCE_EVAL

